import requests
import os 
import json

def lambda_handler(event, context):
    print(event)
    
    Method = os.environ['method']
    HOST = os.environ['host']
    URL = os.environ['url']
   
    
    payload = {'job_id': 1, 'job_name': 34}
    r = requests.get('https://skycode5.riversilica.com/jobs/job_list', params=payload)
    
    print('method:', Method)
    print('host:' ,HOST)
    print('url:', URL)

    print(r.text)
